@import '../css/_common'

// CUSTOM STYLE ////////////////////////////////////////////////////////////////



random(min,max)
	return floor(math(0, 'random')*(max - min + 1) + min)

for num in (1..30)
	.item{num}
		left random(0, 300)px

for num in (1..30)
	.item{num}
		top random(0, 300)px

.process

	.items
		position relative
		top -280px

	.item
		position absolute

.item .panel
	size 3em
	.list-group
	.panel-footer
		display none


